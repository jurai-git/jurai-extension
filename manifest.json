{
    "manifest_version": 2,
    "name": "JurAI",
    "version": "1.0",
    "description": "JurAI - Praticidade em seus casos judiciais",
    "permissions": [
        "cookies",
        "http://jurai-git.github.io/*",
        "https://jurai-git.github.io/*"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon48.png",
        "96": "icon96.png"
      },
      "default_title": "JurAI"
    },
    "icons": {
      "48": "icon48.png",
      "96": "icon96.png"
    }
  }