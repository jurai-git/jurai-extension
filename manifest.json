{
  "manifest_version": 2,
  "name": "JurAI",
  "version": "1.0",
  "description": "JurAI - Praticidade em seus casos judiciais",
  "permissions": [
    "tabs",
    "cookies",
    "http://jurai-git.github.io/",
    "https://jurai-git.github.io/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "./assets/img/jurai-icon.png"
    },
    "default_title": "JurAI"
  },
  "icons": {
    "48": "./assets/img/jurai-icon.png"
  }
}